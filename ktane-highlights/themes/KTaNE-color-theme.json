{
	"$schema": "vscode://schemas/color-theme",
	"name": "KTaNE",
	"type": "dark",
	"colors": {
		"activityBarBadge.background": "#c04040",
		"editor.background": "#1c1814",
		"editor.foreground": "#d4d4d4",
		"editor.inactiveSelectionBackground": "#3a3d41",
		"editor.selectionHighlightBackground": "#add6ff26",
		"editorIndentGuide.activeBackground": "#707070",
		"editorIndentGuide.background": "#404040",
		"input.placeholderForeground": "#a6a6a6",
		"list.dropBackground": "#383b3d",
		"menu.background": "#252526",
		"menu.foreground": "#cccccc",
		"settings.numberInputBackground": "#292929",
		"settings.textInputBackground": "#292929",
		"sideBarTitle.foreground": "#bbbbbb",
		"statusBarItem.remoteBackground": "#16825d",
		"statusBarItem.remoteForeground": "#ffffff",
	},
	"tokenColors": [
		{
			"scope": "meta.serial.letter.consonant",
			"settings": { "foreground": "#C04040" }
		},
		{
			"scope": "meta.serial.letter.vowel",
			"settings": { "foreground": "#A040FF" }
		},
		{
			"scope": "meta.serial.digit.even",
			"settings": { "foreground": "#40C040" }
		},
		{
			"scope": "meta.serial.digit,odd",
			"settings": { "foreground": "#40A0C0" }
		},

		{
			"scope": [ "meta.space.white", ],
			"settings": { "foreground": "#FFFFFF" }
		},
		{
			"scope": [ "meta.space.black", ],
			"settings": { "foreground": "#606060" }
		},


		{
			"scope": [ "constant.language.true", ],
			"settings": { "foreground": "#FFFFFF", "fontStyle": "bold" }
		},
		{
			"scope": [ "constant.language.false", ],
			"settings": { "foreground": "#808080", "fontStyle": "italic" }
		},
		{
			"scope": [ "meta.significantdigit", ],
			"settings": { "foreground": "#FF4040", "fontStyle": "bold" }
		},
		{
			"scope": [ "meta.indicator.lit", ],
			"settings": { "foreground": "#C0A040", "fontStyle": "bold" }
		},
		{
			"scope": [ "meta.indicator.unlit", ],
			"settings": { "foreground": "#6060A0" }
		},
		{
			"scope": [ "colour.red", ],
			"settings": { "foreground": "#FF4040", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.brown", ],
			"settings": { "foreground": "#805020", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.orange", ],
			"settings": { "foreground": "#FFA040", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.yellow", ],
			"settings": { "foreground": "#FFFF40", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.lime", ],
			"settings": { "foreground": "#80FF00", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.green", ],
			"settings": { "foreground": "#00FF00", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.darkgreen", ],
			"settings": { "foreground": "#00A000", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.cyan", ],
			"settings": { "foreground": "#40E0E0", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.blue", ],
			"settings": { "foreground": "#4040FF", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.purple", ],
			"settings": { "foreground": "#A040FF", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.magenta", ],
			"settings": { "foreground": "#FF00FF", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.pink", ],
			"settings": { "foreground": "#FF80FF", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.white", ],
			"settings": { "foreground": "#FFFFFF", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.grey", ],
			"settings": { "foreground": "#808080", "fontStyle": "bold" }
		},
		{
			"scope": [ "colour.black", ],
			"settings": { "foreground": "#404040", "fontStyle": "bold" }
		},

		{
			"scope": "markup.heading",
			"settings": { "foreground": "#569cd6", "fontStyle": "bold underline" }
		},
		{
			"scope": "markup.heading.part",
			"settings": { "foreground": "#569cd6", "fontStyle": "bold" }
		},
		{
			"scope": [ "markup.tableheader", ],
			"settings": { "foreground": "#569cd6", "fontStyle": "bold underline" }
		},
		{
			"scope": "invalid.illegal",
			"settings": { "foreground": "#FF0000", "fontStyle": "underline" }
		},
		{
			"scope": "comment",
			"settings": { "foreground": "#6A9955", "fontStyle": "italic" }
		},
		{
			"scope": "string",
			"settings": { "foreground": "#ce9178" }
		},
		{
			"scope": [
				"support.type.vendored.property-name",
				"support.type.property-name",
				"variable.css",
				"variable.scss",
				"variable.other.less",
				"source.coffee.embedded"
			],
			"settings": { "foreground": "#9cdcfe" }
		},
		{
			"scope": "constant.language",
			"settings": { "foreground": "#569cd6" }
		},
		{
			"scope": "constant.numeric",
			"settings": { "foreground": "#b5cea8" }
		}
	]
}
